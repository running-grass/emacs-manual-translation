#. type: table
#: src/cmdargs.texi:934
msgid "Set the @dfn{color support mode} when Emacs is run on a text terminal.  This option overrides the number of supported colors that the character terminal advertises in its @code{termcap} or @code{terminfo} database.  The parameter @var{mode} can be one of the following:"
msgstr ""

#. type: item
#: src/cmdargs.texi:935
#, no-wrap
msgid "never"
msgstr ""

#. type: itemx
#: src/cmdargs.texi:936
#, no-wrap
msgid "no"
msgstr ""

#. type: table
#: src/cmdargs.texi:939
msgid "Don't use colors even if the terminal's capabilities specify color support."
msgstr ""

#. type: item
#: src/cmdargs.texi:939
#, no-wrap
msgid "default"
msgstr ""

#. type: itemx
#: src/cmdargs.texi:940
#, no-wrap
msgid "auto"
msgstr ""

#. type: table
#: src/cmdargs.texi:944
msgid "Same as when @option{--color} is not used at all: Emacs detects at startup whether the terminal supports colors, and if it does, turns on colored display."
msgstr ""

#. type: item
#: src/cmdargs.texi:944
#, no-wrap
msgid "always"
msgstr ""

#. type: itemx
#: src/cmdargs.texi:945
#, no-wrap
msgid "yes"
msgstr ""

#. type: itemx
#: src/cmdargs.texi:946
#, no-wrap
msgid "ansi8"
msgstr ""

#. type: table
#: src/cmdargs.texi:949
msgid "Turn on the color support unconditionally, and use color commands specified by the ANSI escape sequences for the 8 standard colors."
msgstr ""

#. type: var{#1}
#: src/cmdargs.texi:949
#, no-wrap
msgid "num"
msgstr ""

#. type: table
#: src/cmdargs.texi:959
