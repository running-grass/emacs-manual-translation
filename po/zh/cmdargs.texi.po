#, no-wrap
msgid "--internal-border"
msgstr ""

#. type: cindex
#: src/cmdargs.texi:1116
#, no-wrap
msgid "internal border width, command-line argument"
msgstr ""

#. type: table
#: src/cmdargs.texi:1119
msgid "Specify @var{width} as the width of the internal border (around the frame's text area), in pixels."
msgstr ""

#. type: item
#: src/cmdargs.texi:1120
#, no-wrap
msgid "-bw @var{width}"
msgstr ""

#. type: opindex
#: src/cmdargs.texi:1121
